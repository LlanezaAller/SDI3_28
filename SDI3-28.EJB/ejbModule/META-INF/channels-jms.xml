<messaging-deployment xmlns="urn:jboss:messaging-deployment:1.0">
	<hornetq-server>
		<jms-destinations>
			<jms-queue name="LogQueue">
				<entry name="queue/LogQueue" />
				<entry name="java:jboss/exported/jms/queue/LogQueue" />
				<durable>true</durable>
			</jms-queue>
			<jms-queue name="SDI3-28-ShareMytrip-Sender">
				<entry name="queue/SDI3-28-ShareMytrip-Sender" />
				<entry name="java:jboss/exported/jms/queue/SDI3-28-ShareMytrip-Sender" />
				<durable>true</durable>
			</jms-queue>
			<jms-topic name="SDI3-28-ShareMytrip-Receiver">
				<entry name="topic/SDI3-28-ShareMytrip-Receiver" />
				<entry name="java:jboss/exported/jms/topic/SDI3-28-ShareMytrip-Receiver" />
			</jms-topic>
		</jms-destinations>
	</hornetq-server>
</messaging-deployment>